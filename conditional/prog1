package first;
import java.util.*;
public class pack1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
Scanner sc=new Scanner(System.in);


System.out.println("Enter the numnber: ");
int a=sc.nextInt();

if(!(a%2==0)){
	System.out.println("Wired .odd");
}
 else if((a%2==0)&&(a<50)){
	 System.out.println(" Not Weird.even ");
}
 else if((a%2==0)&&(a>50)){
	 System.out.println(" Not Weird .even");
 }
	}

}
